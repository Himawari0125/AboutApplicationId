<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.slee.aboutapplicationid"
    android:versionCode="2"
    android:versionName="2.2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/pro"
        android:supportsRtl="true"
        android:name=".MyApplication"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.slee.aboutapplicationid.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity-alias
            android:name=".pro"
            android:targetActivity="com.example.slee.aboutapplicationid.MainActivity"
            android:icon="@mipmap/app_logo"
            android:label="@string/pro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name=".free"
            android:targetActivity="com.example.slee.aboutapplicationid.MainActivity"
            android:icon="@mipmap/app_logo"
            android:label="@string/free">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name=".third"
            android:targetActivity="com.example.slee.aboutapplicationid.MainActivity"
            android:icon="@mipmap/ic_launcher_round"
            android:label="@string/third">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity-alias>
    </application>

</manifest>